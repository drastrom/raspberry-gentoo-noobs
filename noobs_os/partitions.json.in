{
    "partitions": [
        {
            "filesystem_type": "FAT",
            "label": "boot",
            "mkfs_options": "-F 32",
            "partition_size_nominal": 69,
            "uncompressed_tarball_size": @@@BOOT_SIZE@@@,
            "want_maximised": false
        },
        {
            "filesystem_type": "ext4",
            "label": "root",
            "mkfs_options": "-O ^huge_file",
            "partition_size_nominal": 2048,
            "uncompressed_tarball_size": @@@ROOT_SIZE@@@,
            "want_maximised": true
        }
    ]
}
